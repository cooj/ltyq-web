{
  "private": true,
  "packageManager": "pnpm@7.29.3",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "lint": "eslint . --fix",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "release-it"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "element-plus": "^2.3.6",
    "release-it": "^16.1.0",
    "swiper": "^9.4.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@element-plus/nuxt": "^1.0.5",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/ep": "^1.1.11",
    "@iconify-json/twemoji": "^1.1.11",
    "@nuxt/devtools": "^0.6.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@pinia/nuxt": "^0.4.11",
    "@types/ws": "^8.5.5",
    "@unocss/nuxt": "^0.53.1",
    "@unocss/preset-rem-to-px": "^0.53.1",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vueuse/nuxt": "^10.2.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.3",
    "pinia": "^2.1.4",
    "prisma": "^4.15.0",
    "sass": "^1.63.4",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged0": {
    "*.vue": [
      "eslint --fix"
    ],
    "*.{htm,html,css,sss,less,scss,sass}": [
      "eslint --fix"
    ]
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}