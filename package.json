{
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@7.29.3",
    "scripts": {
        "dev": "nuxi dev",
        "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
        "lint": "eslint . --fix",
        "build": "nuxi build",
        "start": "node .output/server/index.mjs",
        "postinstall": "nuxi prepare",
        "generate": "nuxi generate",
        "start:generate": "npx serve .output/public",
        "prepare": "husky install",
        "commit": "git-cz",
        "release": "release-it",
        "prisma-push": "pnpx prisma db push"
    },
    "dependencies": {
        "@prisma/client": "^5.1.1",
        "crypto-js": "^4.1.1",
        "element-plus": "^2.3.9",
        "jsonwebtoken": "^9.0.1",
        "release-it": "^16.1.5",
        "swiper": "^9.4.1",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.40.2",
        "@element-plus/nuxt": "^1.0.5",
        "@iconify-json/carbon": "^1.1.19",
        "@iconify-json/ep": "^1.1.12",
        "@iconify-json/ic": "^1.1.14",
        "@iconify-json/twemoji": "^1.1.12",
        "@nuxt/devtools": "^0.8.0",
        "@nuxtjs/color-mode": "^3.3.0",
        "@nuxtjs/i18n": "8.0.0-rc.2",
        "@pinia/nuxt": "^0.4.11",
        "@types/crypto-js": "^4.1.1",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/ws": "^8.5.5",
        "@unocss/eslint-config": "^0.55.0",
        "@unocss/nuxt": "^0.55.0",
        "@unocss/preset-rem-to-px": "^0.55.0",
        "@vite-pwa/nuxt": "^0.1.0",
        "@vueuse/nuxt": "^10.3.0",
        "commitizen": "^4.3.0",
        "cz-git": "^1.7.1",
        "eslint": "^8.47.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.0",
        "nuxt": "^3.6.5",
        "pinia": "^2.1.6",
        "prisma": "^5.1.1",
        "sass": "^1.65.1",
        "typescript": "^5.1.6"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged0": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{htm,html,css,sss,less,scss,sass}": [
            "eslint --fix"
        ]
    },
    "lint-staged": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
